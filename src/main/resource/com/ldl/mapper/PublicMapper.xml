<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ldl.mapper.PublicMapper"><!--这个是-->
    <insert id="insertPublic">
        insert into tbl_admin_public(title,public_time,content) value(#{title},#{public_time},#{content})
    </insert>
    <select id="selectAllAdminPublic" resultType="com.ldl.bean.AdminPublic">
        select * from tbl_admin_public
    </select>


</mapper>